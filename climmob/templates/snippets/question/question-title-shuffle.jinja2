{% set fatype = "fa-keyboard-o" %}
{% set qtype = "Text input" %}
{% set others = 0 %}
{% set qlabel = "label-primary" %}
{% if question.question_overall == 1 or question.question_overallperf == 1 %}
    {% set overall = 1 %}
{% else %}
    {% set overall = 0 %}
{% endif %}

